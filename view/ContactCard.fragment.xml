<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<QuickView id="idQuickView">
		<QuickViewPage description="{/AdvPosition}" header="{i18n>ADVISOR}" icon="{/PhotoUri}" id="idQuickViewPage" title="{/Name}">
			<QuickViewGroup heading="{i18n>CONTACT_DETAILS}" id="idGroup1">
				<QuickViewGroupElement id="idQuickViewMial" label="{i18n>EMAIL_ID}" type="email" value="{/EmailId}"></QuickViewGroupElement>
				<QuickViewGroupElement id="idQuickViewMob" label="{i18n>MOBILE}" type="phone" value="{/MobileNum}"></QuickViewGroupElement>
			</QuickViewGroup>
			<QuickViewGroup heading="{i18n>UNIVERSITY}" id="idGroup2">
				<QuickViewGroupElement id="idQuickViewName" label="{i18n>NAME}" value="{/OrgUnit}"></QuickViewGroupElement>
				<QuickViewGroupElement id="idQuickViewDept" label="{i18n>DEPARTMENT}" value="{/Department}"></QuickViewGroupElement>
			</QuickViewGroup>
		</QuickViewPage>
	</QuickView>
</core:FragmentDefinition>